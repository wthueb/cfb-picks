{
  "$schema": "https://json.schemastore.org/tsconfig",
  "extends": "./base.json",
  "compilerOptions": {
    /** Emit types and JavaScript for internal packages */
    "declaration": true,
    "declarationMap": true,
    "emitDeclarationOnly": false,
    "noEmit": false,
    "outDir": "${configDir}/dist",
    "module": "ES2022",
    "target": "ES2022"
  }
}
